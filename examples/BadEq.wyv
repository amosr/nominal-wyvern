name Eq {z =>
  type E >= Bot
}
name List {z =>
  type T <= Top
  type E = List {type T <= Eq {type E >= z.T}}
}
subtype List <: Eq
name Tree {z =>
  type T = Tree
  type E = List {type T <= Eq {type E >= z.T}}
}
subtype Tree <: List

//BAD
let bad : Eq {type E >= Tree} = new Tree {this =>
  type T = Tree
  type E = List {type T <= Eq {type E >= this.T}}
}
in bad
